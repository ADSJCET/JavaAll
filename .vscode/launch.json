  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "enter program name, for example ${workspaceFolder}/a.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/path/to/gdb",
      "setupCommands": [
        {
            "type": "java",
            "name": "Launch BasicProblems2",
            "request": "launch",
            "mainClass": "dsa.BasicProblems2",
            "projectName": "code-demo_ec8955a4"
        },
        {
            "type": "java",
            "name": "Launch BasicProblems3",
            "request": "launch",
            "mainClass": "dsa.BasicProblems3",
            "projectName": "code-demo_ec8955a4"
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ]
